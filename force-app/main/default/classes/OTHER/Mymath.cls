public with sharing class Mymath {



    Public Integer method (Integer num1, Integer num2) {

        return num1+num2;

    }

    Public Decimal method(Integer num1 , Decimal dec1, Integer num2 ) {
        return num1+dec1+num2;

    }
    Public Decimal method(Integer num1 , Decimal dec1, Integer num2, Decimal dec2 ) {

        return num1+dec1+num2+dec2;

    }

}